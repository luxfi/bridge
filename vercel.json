{
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "ENABLE_EXPERIMENTAL_PNPM": "true",
      "ENABLE_EXPERIMENTAL_COREPACK": "1"
    }
  },
  "buildCommand": "pnpm run build && npx prisma generate",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install --no-frozen-lockfile"
}
